@page
@{
    ViewData["Title"] = "Board";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<script>window.addEventListener('load', () => {

        CallData();

    });
    function CallData() {
        fetch(baseUrl + "/api/User/GetDataAllOfLoginUser", {
            method: 'GET',
            credentials: 'include',
            headers: {
                'Content-Type': 'application/json',
            },
            redirect: 'follow'
        }).then(response => {
            if (response.ok) {
            } else if (response.status == 401) {
                window.location.href = "/User/Login"
            } else {
                console.log(response.status)
            }
        }
        ).catch((e) => {
        });
    }</script>
<h1 class="h3 mb-2 text-gray-800">@ViewData["Title"]</h1>

<p class="mb-4 note">

</p>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">BoardList</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive table-striped">
            <table class="table table-bordered" id="hrList" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th class="text-center bg-warning">#</th>
                        <th class="text-center bg-warning">
                            Title
                        </th>
                        <th class="text-center bg-warning">
                            UserName
                        </th>
                        <th class="text-center bg-warning">
                            UpdatedAt
                        </th>
                        <th class="text-center bg-warning">
                            ReadCount
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td colspan="7">Data Not Exist</td>
                    </tr>
                </tbody>
                
            </table>
        </div>
    </div>
</div>